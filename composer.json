{
    "name": "pierres/symfony-database-test",
    "description": "Symfony WebTestCase with database support",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre Schmitz",
            "email": "mail@pierre-schmitz.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7",
        "symfony/browser-kit": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/proxy-manager-bridge": "^5.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "SymfonyDatabaseTest\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.4.99"
        }
    }
}
